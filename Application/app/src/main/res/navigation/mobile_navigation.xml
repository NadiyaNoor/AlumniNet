<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_forum">

    <fragment
        android:id="@+id/navigation_forum"
        android:name="com.example.alumninet.ui.forums.ForumsFragment"
        android:label="@string/title_forums"
        tools:layout="@layout/fragment_forum" />

    <fragment
        android:id="@+id/navigation_connections"
        android:name="com.example.alumninet.ui.connections.ConnectionsFragment"
        android:label="@string/title_connections"
        tools:layout="@layout/fragment_connections" />

    <fragment
        android:id="@+id/navigation_my_profile"
        android:name="com.example.alumninet.ui.myProfile.MyProfileFragment"
        android:label="@string/title_my_profile"
        tools:layout="@layout/fragment_my_profile" />
